<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.AdminJadwalActivity"
    android:background="@color/bluee">
    <include layout="@layout/label_admin"/>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/logosplash"
        android:backgroundTint="@color/cyan"
        android:backgroundTintMode="multiply"
        android:padding="285dp">
    </RelativeLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rjadwal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginVertical="70dp"
        tools:listitem="@layout/card_item_admin2" />

</RelativeLayout>